<?xml version="1.0"?>
<extension key="coop.symbiotic.mailingpresets" type="module">
  <file>mailingpresets</file>
  <name>Mailing Presets</name>
  <description>Creates a new CiviCRM mailing using presets.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/coopsymbiotic/coop.symbiotic.mailingpresets</url>
    <url desc="Documentation">https://github.com/coopsymbiotic/coop.symbiotic.mailingpresets</url>
    <url desc="Support">https://github.com/coopsymbiotic/coop.symbiotic.mailingpresets</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-04</releaseDate>
  <version>1.1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.73</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Mailingpreset</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Mailingpreset_Upgrader</upgrader>
</extension>
